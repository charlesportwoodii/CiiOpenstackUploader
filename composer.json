{
    "name": "ciims-plugins/rackspaceuploader",
    "description": "A CiiMS plugin to upload files to Rackspace Cloud files CDN",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles R. Portwood II",
            "email": "charlesportwoodii@ethreal.net",
            "homepage": "https://www.erianna.com",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/charlesportwoodii/CiiOpenstackUploader/issues",
        "source": "https://github.com/charlesportwoodii/CiiOpenstackUploader"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ircmaxell/PHP-CryptLib"
        }
    ],
    "require": {
        "rackspace/php-opencloud": "v1.12.1",
        "CryptLib/CryptLib": "dev-master"
    },
    "autoload": {
        "classmap": [
            "CiiOpenCloud.php",
            "CiiOpenstackUploader.php"
        ],
        "psr-0": {
            "Detection": "namespaced/"
        }
    }
}
